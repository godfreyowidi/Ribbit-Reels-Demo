{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
  "DefaultConnection": "${DefaultConnection}"
  },
  "Jwt": {
    "Key": "${Jwt__Key}",
    "Issuer": "RibbitReels",
    "Audience": "RibbitReelsUsers",
    "ExpireMinutes": 60
  },
  "GoogleAuth": {
    "ClientId": "${GoogleAuth__ClientId}",
    "ClientSecret": "${GoogleAuth__ClientSecret}"
  },
  "Authentication": {
    "TestUser": {
      "UserId": "${Authentication__TestUser__UserId}",
      "Email": "${Authentication__TestUser__Email}",
      "Name": "${Authentication__TestUser__Name}",
      "Role": "${Authentication__TestUser__Role}"
    }
  }
}
